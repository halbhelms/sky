#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/halhelms/sky/node_modules/.pnpm/@sveltejs+package@2.3.1_svelte@4.2.13_typescript@5.4.5/node_modules/@sveltejs/package/node_modules:/Users/halhelms/sky/node_modules/.pnpm/@sveltejs+package@2.3.1_svelte@4.2.13_typescript@5.4.5/node_modules/@sveltejs/node_modules:/Users/halhelms/sky/node_modules/.pnpm/@sveltejs+package@2.3.1_svelte@4.2.13_typescript@5.4.5/node_modules:/Users/halhelms/sky/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/halhelms/sky/node_modules/.pnpm/@sveltejs+package@2.3.1_svelte@4.2.13_typescript@5.4.5/node_modules/@sveltejs/package/node_modules:/Users/halhelms/sky/node_modules/.pnpm/@sveltejs+package@2.3.1_svelte@4.2.13_typescript@5.4.5/node_modules/@sveltejs/node_modules:/Users/halhelms/sky/node_modules/.pnpm/@sveltejs+package@2.3.1_svelte@4.2.13_typescript@5.4.5/node_modules:/Users/halhelms/sky/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-package.js" "$@"
else
  exec node  "$basedir/../../svelte-package.js" "$@"
fi
